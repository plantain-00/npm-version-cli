<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="cn.arkie.ps" ExtensionBundleVersion="1.0.0" Version="7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="cn.arkie.ps.upload-layer-panel" Version="1.5.2" />
    <Extension Id="cn.arkie.ps.design-assets-panel" Version="1.5.2" />
    <!-- <Extension Id="cn.arkie.ps.kv-marker-panel" Version="1.5.2" /> -->
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PHSP" Version="15" />
      <Host Name="PHXS" Version="15" />
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="7.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="cn.arkie.ps.upload-layer-panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/index.jsx</ScriptPath>
          <CEFCommandLine>
              <Parameter>--enable-nodejs</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Arkie - Upload Layer</Menu>
          <Geometry>
            <Size>
              <Height>500</Height>
              <Width>300</Width>
            </Size>
          </Geometry>
          <Icons>
              <Icon Type="Normal">./resources/light@2x.png</Icon>
              <Icon Type="DarkNormal">./resources/dark@2x.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
    <Extension Id="cn.arkie.ps.design-assets-panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/index.jsx</ScriptPath>
          <CEFCommandLine>
              <Parameter>--enable-nodejs</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Arkie - Design Assets</Menu>
          <Geometry>
            <Size>
              <Height>817</Height>
              <Width>640</Width>
            </Size>
          </Geometry>
          <Icons>
              <Icon Type="Normal">./resources/light@2x.png</Icon>
              <Icon Type="DarkNormal">./resources/dark@2x.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
    <Extension Id="cn.arkie.ps.kv-marker-panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/index.jsx</ScriptPath>
          <CEFCommandLine>
              <Parameter>--enable-nodejs</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Arkie - KV Marker</Menu>
          <Geometry>
            <Size>
              <Height>500</Height>
              <Width>300</Width>
            </Size>
          </Geometry>
          <Icons>
              <Icon Type="Normal">./resources/light@2x.png</Icon>
              <Icon Type="DarkNormal">./resources/dark@2x.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
